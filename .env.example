# Discord設定（ブラウザ自動化用）
# Discordアカウントのメールアドレス
DISCORD_EMAIL=your_discord_email@example.com

# Discordアカウントのパスワード
DISCORD_PASSWORD=your_discord_password

# 取得したいチャンネルのURL
DISCORD_CHANNEL_URL=https://discord.com/channels/xxx/yyy

# Claude API設定
ANTHROPIC_API_KEY=sk-ant-xxx

# LINE Messaging API設定
# LINE Developersコンソールで発行したチャネルアクセストークン
LINE_CHANNEL_ACCESS_TOKEN=your_channel_access_token

# LINE DevelopersコンソールのBasic settingsに表示されるYour user ID
LINE_USER_ID=Uxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
